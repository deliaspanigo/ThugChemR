output <- list()
# 1) Original Output Solver
output[[1]] <- the_solver
names(output)[1] <- "Level01_Solver"
# 2) Add Element and Oxygen in reactives and products
output[[2]] <- cbind.data.frame(the_solver[1], vector_symbol,
the_solver[c(2,3)], vector_oxygen,
