#                                  element_valence = element_valence,
#                                  gas_status_element = gas_status_element,
#                                  language = language)$ChemFormule_LaTeX
output[[9]] <- output[[6]][nrow(output[[6]]), ncol(output[[6]])]
names(output)[9] <- "ChemFormule_LaTeX"
output[[9]]
